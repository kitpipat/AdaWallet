        "NameInSource": "Sale",
        "SqlCommand": "declare @ptDocNo varchar(20) = '{SP_tDocNo}' \r\n declare @pnLanguage int = '{nLanguage}' \r\n declare @ptDocBch varchar(5) = '{SP_tDocBch}' \r\n SELECT CONVERT(VARCHAR(10),FDRdhDStart,103) AS FTRdhDStart,CONVERT(VARCHAR(10),FDRdhDStop,103) AS FTRdhDStop,\r\n\tFORMAT(FDRdhTStart,'HH:mm:ss') AS FTRdhTStart,FORMAT(FDRdhTStop,'HH:mm:ss') AS FTRdhTStop,FTRdhStaClosed,\r\n\tHD.FTBchCode,BchL.FTBchName, HD.FTRdhDocNo ,FTRdhDocType,FTRdhCalType,\r\n\tFDRdhDocDate ,CONVERT(VARCHAR(10),FDRdhDocDate,103) AS FTXshDocDate,CONVERT(VARCHAR(10),FDRdhDocDate,8) AS FTXshDocTime,\r\n\tHDL.FTRdhName,HDL.FTRdhNameSlip,\r\n\tFTRdhStaOnTopPmt,FNRdhLimitQty,\r\n\t HD.FTUsrCode,UsrL.FTUsrName, \r\n\tISNULL(FTRdhUsrApv,'') AS FTXshApvCode,ISNULL(UsrApp.FTUsrName,'') AS AppName,  \r\n\tFTCreateBy,FDCreateOn,  FTRdhStaDoc,FTRdhStaPrcDoc,\r\n\tFTRdhStaApv,FNRdhStaDocAct\r\nFROM   TARTRedeemHD HD WITH (NOLOCK) \r\nLEFT JOIN TARTRedeemHD_L HDL WITH (NOLOCK)  ON HD.FTBchCode = HDL.FTBchCode AND HD.FTRdhDocNo = HDL.FTRdhDocNo AND HDL.FNLngID = @pnLanguage\r\nLEFT JOIN  TCNMUser_L UsrL ON HD.FTUsrCode = UsrL.FTUsrCode  AND UsrL.FNLngID = @pnLanguage\r\nLEFT JOIN  TCNMUser_L UsrApp ON HD.FTRdhUsrApv = UsrApp.FTUsrCode  AND UsrApp.FNLngID = @pnLanguage\r\nLEFT JOIN  TCNMBranch_L BchL ON HD.FTBchCode = BchL.FTBchCode  AND BchL.FNLngID = @pnLanguage\r\nWHERE HD.FTRdhDocNo = @ptDocNo AND HD.FTBchCode = @ptDocBch\r\n",
