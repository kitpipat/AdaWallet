        "NameInSource": "Vending",
        "CommandTimeout": 1000,
        "SqlCommand": "declare @ptDocNo varchar(20) = '{SP_tDocNo}' \r\n declare @pnLanguage int = '{nLanguage}' \r\n declare @ptDocBch varchar(5) = '{SP_tDocBch}' \r\n SELECT HD.FTBchCode,Bch.FTBchName, HD.FTXphDocNo, HD.FTXphDocType,\r\nCASE WHEN HD.FTXphDocType = '1' THEN 'Base Price' ELSE 'Price Off' END AS  FTXphDocTypeName,\r\nCASE HD.FTXphStaAdj\r\nWHEN '1' THEN 'ราคาใหม่'\r\nWHEN '2' THEN 'ปรับลด %'\r\nWHEN '3' THEN 'ปรับลด มูลค่า'\r\nWHEN '4' THEN ' ปรับเพิ่ม % '\r\nWHEN '5' THEN 'ปรับเพิ่ม มูลค่า'\r\nEND\r\nAS FTXphStaAdjTH,\r\nCASE HD.FTXphStaAdj\r\nWHEN '1' THEN 'New Price'\r\nWHEN '2' THEN 'Discount %'\r\nWHEN '3' THEN 'Discount Values'\r\nWHEN '4' THEN 'Charge %'\r\nWHEN '5' THEN 'Charge Values'\r\nEND\r\nAS FTXphStaAdjEng,\r\n\r\nHD.FTXphStaAdj,FDXphDStart,CONVERT(VARCHAR(10),HD.FDXphDStart,103) AS FTXphDStart,HD.FTXphTStart,\r\nHD.FDXphDStop,CONVERT(VARCHAR(10),HD.FDXphDStop,103) AS FTXphDStop,HD.FTXphTStop,\r\n  HD.FDXphDocDate,CONVERT(VARCHAR(10),HD.FDXphDocDate,103) AS FTDocDate,FTXphDocTime , HD.FTUsrCode, Usr.FTUsrName,\r\nHD.FTXphStaApv,HD.FTXphUsrApv,UsrA.FTUsrName AS FTUsrNameApp, Dpl.FTDptName,\r\n  HD.FDLastUpdOn, HD.FTLastUpdBy, ISNULL(HD.FTXphStaPrcDoc,'') AS FTXphStaPrcDoc,ISNULL(FTXphStaDoc,'') AS FTXphStaDoc\r\nFROM TCNTPdtAdjPriHD HD WITH (NOLOCK) LEFT OUTER JOIN\r\n  TCNMUser_L Usr WITH (NOLOCK) ON HD.FTUsrCode = Usr.FTUsrCode AND Usr.FNLngID = @pnLanguage LEFT OUTER JOIN\r\n  TCNMBranch_L Bch WITH (NOLOCK) ON HD.FTBchCode = Bch.FTBchCode AND Bch.FNLngID = @pnLanguage LEFT OUTER JOIN\r\n  TCNMUser_L UsrA WITH (NOLOCK) ON HD.FTXphUsrApv = UsrA.FTUsrCode AND UsrA.FNLngID = @pnLanguage LEFT OUTER JOIN \r\n  TCNMUser UsrD WITH (NOLOCK) ON HD.FTUsrCode = UsrD.FTUsrCode LEFT OUTER JOIN\r\n  TCNMUsrDepart_L Dpl WITH (NOLOCK) ON Dpl.FTDptCode = UsrD.FTDptCode AND Dpl.FNLngID = @pnLanguage\r\nWHERE  HD.FTXphDocNo = @ptDocNo AND HD.FTBchCode = @ptDocBch",
